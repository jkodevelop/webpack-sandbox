{
  "name": "webpack-sandbox",
  "version": "1.0.0",
  "description": "webpack sandbox testing",
  "private": true,
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "webpack --config webpack.config.js --env production --env scripts=start",
    "build:dev": "webpack --config webpack.dev.js --env development --env scripts=builddev",
    "build": "webpack --config webpack.prod.js --env production --env scripts=build",
    "watch": "webpack --watch --config webpack.prod.js --env production --env scripts=watch",
    "serve": "webpack serve --open --config webpack.dev.js --env development --env scripts=serve"
  },
  "keywords": [
    "webpack",
    "starter",
    "sandbox",
    "test"
  ],
  "author": "Jason Ko",
  "license": "MIT",
  "devDependencies": {
    "browser-sync": "^2.29.3",
    "browser-sync-webpack-plugin": "^2.3.0",
    "copy-webpack-plugin": "^13.0.0",
    "css-loader": "^7.1.2",
    "css-minimizer-webpack-plugin": "^7.0.2",
    "csv-loader": "^3.0.5",
    "dotenv-webpack": "^8.1.0",
    "html-webpack-plugin": "^5.6.3",
    "mini-css-extract-plugin": "^2.9.2",
    "sass": "^1.89.0",
    "sass-loader": "^16.0.5",
    "style-loader": "^4.0.0",
    "webpack": "^5.99.8",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.1",
    "webpack-merge": "^6.0.1",
    "xml-loader": "^1.2.1",
    "yamljs": "^0.3.0"
  },
  "dependencies": {
    "lodash": "^4.17.21"
  }
}
